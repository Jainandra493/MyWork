                    <property
                      dataType="System.String"
                      description="The SQL command to be executed."
                      expressionType="Notify"
                      name="SqlCommand">SELECT	tch.cedcnid AS DCN,
   tch.clearinghouseclaimid  AS ICN ,  (
		CASE   
			WHEN tch.claimtype = '3' THEN "Medical" 
			WHEN tch.claimtype = '2' THEN "Hospital"  
		END) AS  MedicalHospital,
		(CASE 
			WHEN tch.claimsubmissiontype = '01' THEN "Paper" 
			WHEN  tch.claimsubmissiontype = '03'  THEN "Manual" 
		END) AS PaperManual,
		tce.cirrusreceiptdate as CirrusLoadDate   
FROM	uhc_pfv_prd.claimheader_std AS tch 
INNER JOIN uhc_pfv_prd.claimevent_std AS tce 
	ON  tch.chclaimid = tce.claimid 
WHERE	
tce.cirrusreceiptdate &gt;=  date_sub(FROM_UNIXTIME(UNIX_TIMESTAMP(),'yyyy-MM-dd'), 7) and tch.cedcnid &lt;&gt; ''
--CAST(tce.cirrusreceiptdate AS DATE) &gt;= Cast(date_sub(cast(current_timestamp as date), 7)  as Date) 
GROUP BY tch.cedcnid,
		tch.clearinghouseclaimid,
		CASE 
			WHEN tch.claimtype = '3' THEN "Medical"  
			WHEN tch.claimtype = '2' THEN "Hospital"  
		END, 
		CASE 
			WHEN tch.claimsubmissiontype = '01' THEN "Paper"  
			WHEN tch.claimsubmissiontype = '03' THEN "Manual" 
		END, tce.cirrusreceiptdate,
		tch.claimtypedesc,
		 tch.claimsubmissiontypedesc,
		tch.claimenvelopetype 
HAVING	(((
		CASE 
			WHEN tch.claimtype = '3' THEN "Medical"  
			WHEN tch.claimtype = '2' THEN "Hospital"  
		END)= "Medical" 
	Or(
		CASE 
			WHEN tch.claimtype = '3' THEN "Medical"  
			WHEN tch.claimtype = '2' THEN "Hospital"  
		END) = "Hospital") 
	AND ((
		CASE 
			WHEN tch.claimsubmissiontype = '01'  THEN "Paper" 
			WHEN  tch.claimsubmissiontype = '03' THEN "Manual" 
		END) = "Paper" 
	Or  (
		CASE 
			WHEN tch.claimsubmissiontype = '01' THEN "Paper"  
			WHEN  tch.claimsubmissiontype = '03' THEN "Manual" 
		END)  = "Manual") 
	AND((tch.claimenvelopetype) &lt;&gt; "4")) 

Union All
SELECT	tch.submittertracenum AS DCN,
   tch.clearinghouseclaimid  AS ICN ,  (
		CASE   
			WHEN tch.claimtype = '3' THEN "Medical" 
			WHEN tch.claimtype = '2' THEN "Hospital"  
		END) AS  MedicalHospital,
		(CASE 
			WHEN tch.claimsubmissiontype = '01' THEN "Paper" 
			WHEN  tch.claimsubmissiontype = '03'  THEN "Manual" 
		END) AS PaperManual,
		tce.cirrusreceiptdate as CirrusLoadDate   
FROM	uhc_pfv_prd.claimheader_std AS tch 
INNER JOIN uhc_pfv_prd.claimevent_std AS tce 
	ON  tch.chclaimid = tce.claimid 
WHERE	
tce.cirrusreceiptdate &gt;=  date_sub(FROM_UNIXTIME(UNIX_TIMESTAMP(),'yyyy-MM-dd'), 7) and tch.submittertracenum &lt;&gt; ''
--CAST(tce.cirrusreceiptdate AS DATE) &gt;= Cast(date_sub(cast(current_timestamp as date), 7)  as Date) 
GROUP BY tch.submittertracenum,
		tch.clearinghouseclaimid,
		CASE 
			WHEN tch.claimtype = '3' THEN "Medical"  
			WHEN tch.claimtype = '2' THEN "Hospital"  
		END, 
		CASE 
			WHEN tch.claimsubmissiontype = '01' THEN "Paper"  
			WHEN tch.claimsubmissiontype = '03' THEN "Manual" 
